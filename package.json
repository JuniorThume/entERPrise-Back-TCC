{
  "name": "stok-service",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "npx tsc",
    "start": "node build/server.js",
    "dev": "ts-node-dev src/shared/server.ts",
    "prettier-format": "prettier --config .prettierrc --write src/**/*.ts",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "ts-node": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules/*  -- src/shared/server.ts",
    "typeorm-create-migration": "npx typeorm migration:create",
    "typeorm-run-migrations": "npx typeorm-ts-node-commonjs migration:run -d ./src/shared/typeorm/dataSource.ts",
    "typeorm-revert-migrations": "npx typeorm-ts-node-commonjs migration:revert -d ./src/shared/typeorm/dataSource.ts"
  },
  "author": "Valmir Thume Junior",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.6",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.1",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "celebrate": "^15.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20"
  }
}
